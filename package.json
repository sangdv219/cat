  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development npm run start -- --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sonar": "sonar-scanner -Dsonar.token=$SONAR_TOKEN",
    "sequelize:generate": "npx sequelize-cli migration:generate --name $npm_config_name",
    "sequelize:run": "npx sequelize-cli db:migrate",
    "sequelize:revert-file": "npx sequelize-cli db:migrate:undo --name --env development",
    "worker:email": "npx ts-node -r tsconfig-paths/register src/workers/email.worker.ts",
    "worker:order": "npx ts-node -r tsconfig-paths/register src/workers/order.worker.ts"
  },
